generator client {
  provider = "prisma-client-js"
  output   = "../src/generated"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String

  @@map("user")
}

model Car {
  id        Int     @id @default(autoincrement())
  category  String
  brand     String
  model     String
  year      Int
  price     Float
  fueltype  String

  @@map("car")
}
